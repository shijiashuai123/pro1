/* pages/upload/index.wxss */
/* 选择品牌 */
.brandBox {
  width: 100%;
  margin-top: 30rpx;
}
.brandList {
  width: 100%;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.brandItem {
  width: 140rpx;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
  background: #fff;
  font-size: 28rpx;
  color: #333;
  border-radius: 8rpx;
  box-shadow: 0 0 0 #aaa;
  border: 1rpx solid #aaa;
  margin: 40rpx;
}
/* index选择器 */
.indexSelBox {
  /* position: fixed; */
